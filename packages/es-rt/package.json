{
  "name": "@effectful/es-rt",
  "version": "0.0.4",
  "description": "Runtime for @effectful/es",
  "main": "index.js",
  "module": "main.js",
  "scripts": {
    "test": "mocha",
    "test-opt": "cross-env env-cmd $EJS_ENV mocha",
    "configure": "env-cmd configure,default babel src --out-dir .",
    "configure-opt": "cross-env env-cmd $EJS_ENV babel src --out-dir .",
    "lint": "eslint .",
    "watch-bundle": "env-cmd configure,default rollup -c -w",
    "watch-bundle-opt": "cross-env env-cmd configure,$EJS_ENV rollup -c -w",
    "build": "npm run configure && rollup -c",
    "build-bundle-opt": "cross-env env-cmd configure,$EJS_ENV rollup -c",
    "prepare": "npm run build"
  },
  "keywords": [
    "generators",
    "await",
    "async",
    "monad",
    "effectful"
  ],
  "author": "Vitaliy Akimov <vitaliy.akimov@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "cross-env": "^5.1.1",
    "env-cmd": "^7.0.0",
    "eslint": "^4.10.0",
    "mocha": "^4.0.1",
    "rollup": "^0.50.0"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.1"
  }
}
