{
  "env": {
    "development": {
      "plugins": [["transform-es2015-modules-commonjs",
                   {"loose":true,"noInterop":true}]]
    },
    "loose": {
      "passPerPreset": true,
      "presets": [
        ["es-dev",{"loose":true,
                   "all": {
                     "jsExceptions":false,
                     "inlineJsExceptions":false,
                     "inlineThrow":true
                   },
                  }],
        {
          "plugins": [["transform-es2015-modules-commonjs",
                       {"loose":true,"noInterop":true}]]
        }
      ]
    },
    "loose-for-ofs": {
      "passPerPreset": true,
      "presets": [
        ["es-dev",{"loose":true,
                   "invertForOf":true,
                   "all": {
                     "jsExceptions":false,
                     "inlineJsExceptions":false,
                     "inlineThrow":true
                   }
                  }],
        {
          "plugins": [["transform-es2015-modules-commonjs",
                       {"loose":true,"noInterop":true}]]
        }
      ]
    }
  }
}

