{
  "env": {
    "development": {
      "plugins": [["transform-es2015-modules-commonjs",
                   {"loose":true,"noInterop":true}]]
    },
    "loose": {
      "passPerPreset": true,
      "presets": [
        ["es-dev",{"loose":true,
                   "all": {
                     "jsExceptions":false,
                     "inlineJsExceptions":false,
                     "inlineThrow":true,
                     "loopBlockScoping":false
                   },
                  }],
        {
          "plugins": [["transform-es2015-modules-commonjs",
                       {"loose":true,"noInterop":true}]]
        }
      ]
    },
    "loose-defunct-top": {
      "passPerPreset": true,
      "presets": [
        ["es-dev",{"loose":true,
                   "defunct":true,
                   "topLevel":true,
                   "importRT":"@effectful/es-rt/opts/defunct-loose",
                   "loopBlockScoping":false,
                   "all": {
                     "jsExceptions":false,
                     "inlineJsExceptions":false,
                     "inlineThrow":true
                   },
                  }],
        {
          "plugins": [["transform-es2015-modules-commonjs",
                       {"loose":true,"noInterop":true}]]
        }
      ]
    },
    "loose-for-ofs": {
      "passPerPreset": true,
      "presets": [
        ["es-dev",{"loose":true,
                   "invertForOf":true,
		   "importRT":"@effectful/es-rt/opts/loose-delegate",
                   "loopBlockScoping":false,
                   "all": {
                     "jsExceptions":false,
                     "inlineJsExceptions":false,
                     "jsTailCalls":true,
                     "inlineThrow":true,
                     "inlineScopeOp":"context"
                   }
                  }],
        {
          "plugins": [["transform-es2015-modules-commonjs",
                       {"loose":true,"noInterop":true}]]
        }
      ]
    }
  }
}

