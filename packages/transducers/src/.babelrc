{
  "env": {
    "development": {
      "plugins": [["transform-es2015-modules-commonjs",
                   {"loose":true,"noInterop":true}]]
    },
    "loose": {
      "passPerPreset": true,
      "presets": [
        ["es-dev",{"loose":true,
                   "all": {
                     "inlineYieldOp":false,
                     "invertForOf":true,
                     "jsExceptions":false,
                     "inlineJsExceptions":false,
                     "jsTailCalls":true,
                     "markRepeat":false,
                     "contextBy":"reference",
                     "stateStorageField":"$"
                   },
                  }],
        {
          "plugins": [["transform-es2015-modules-commonjs",
                       {"loose":true,"noInterop":true}]]
        }
      ]
    }
  }
}

