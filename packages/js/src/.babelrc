{
  "env": {
    "development": {
      "plugins": [["transform-es2015-modules-commonjs",
                   {"loose":true,"noInterop":true}]]
    },
    "production": {
      "passPerPreset": true,
      "presets": [
        ["@effectful/es-loose",{"jsExceptions":false,
                                "inlineJsExceptions":false,
                                "inlineThrow":true,
                                "loose":true,
                                "moduleAliases":{
                                  "estransducers":"estransducers/loose",
                                  "estransducers/kit":"estransducers/loose/kit",
                                  "estransducers/rt":"estransducers/loose/rt",
                                  "estransducers/dump":"estransducers/loose/dump",
                                  "estransducers/match":"estransducers/loose/match"
                                }}],
        {
          "plugins": [["transform-es2015-modules-commonjs",
                       {"loose":true,"noInterop":true}]]
        }
      ]
    }
  }
}

