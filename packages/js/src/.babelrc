{
  "env": {
    "development": {
      "plugins": [["transform-es2015-modules-commonjs",
                   {"loose":true,"noInterop":true}]]
    },
    "loose": {
      "passPerPreset": true,
      "presets": [
        ["es-dev",
         {"loose":true,
          "all": {
            "jsExceptions":false,
            "inlineJsExceptions":false,
            "inlineThrow":true,
            "moduleAliases":{
              "@effectful/transducers":"@effectful/transducers-loose",
              "@effectful/transducers/kit":"@effectful/transducers-loose/kit",
              "@effectful/transducers/rt":"@effectful/transducers-loose/rt",
              "@effectful/transducers/dump":"@effectful/transducers-loose/dump",
              "@effectful/transducers/match":"@effectful/transducers-loose/match"
            }
          }
         }],
        {
          "plugins": [["transform-es2015-modules-commonjs",
                       {"loose":true,"noInterop":true}]]
        }
      ]
    },
    "loose-for-ofs": {
      "passPerPreset": true,
      "presets": [
        ["es-dev",
         {"loose":true,
          "invertForOf":true,
	        "importRT":"@effectful/es-rt/opts/loose-delegate",
          "all": {
            "jsExceptions":false,
            "inlineJsExceptions":false,
            "jsTailCalls":true,
            "inlineThrow":true,
            "moduleAliases":{
              "@effectful/transducers":"@effectful/transducers-loose",
              "@effectful/transducers/kit":"@effectful/transducers-loose/kit",
              "@effectful/transducers/rt":"@effectful/transducers-loose/rt",
              "@effectful/transducers/dump":"@effectful/transducers-loose/dump",
              "@effectful/transducers/match":"@effectful/transducers-loose/match"
            }
          },
          "effectful": {
            "jsExceptions":false,
            "inlineJsExceptions":false,
            "inlineYieldOp":false,
            "invertForOf":true,
            "jsTailCalls":true,
            "markRepeat":true,
            "contextBy":"reference",
            "inlineThrow":true,
         	  "inlineScopeOp":"context"
          }
         }],
        {
          "plugins": [["transform-es2015-modules-commonjs",
                       {"loose":true,"noInterop":true}]]
        }
      ]
    }
  }
}

