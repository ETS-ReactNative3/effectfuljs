{
  "passPerPreset": true,
  "presets": [
    {
      "plugins": [
        [
          "@babel/transform-modules-commonjs",
          { "noInterop": true, "loose": true }
        ],
        "@babel/transform-arrow-functions"
      ]
    },
    {
      "plugins": [
        [
          "../../../transform",
          { "importRT": "../../../api", "srcRoot": true, "timeTravel": true }
        ]
      ]
    }
  ]
}
