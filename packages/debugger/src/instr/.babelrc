{
  "passPerPreset": true,
  "presets": [
    {
      "plugins": [
        [
          "../../transform",
          {
            "blackbox": true,
            "pureModule": true,
            "importRT": "../kit",
            "srcRoot": true
          }
        ]
      ]
    },
    {
      "plugins": [
        [
          "@babel/transform-modules-commonjs",
          { "loose": true, "noInterop": true }
        ]
      ]
    }
  ]
}
