{
  "passPerPreset": true,
  "presets": [
    {
      "plugins": [
        "@babel/transform-typescript",
        [
          "@babel/transform-modules-commonjs",
          {
            "loose": true,
            "noInterop": true
          }
        ],
        [
          "@babel/transform-destructuring",
          {
            "useBuiltIns": true
          }
        ]
      ]
    },
    {
      "plugins": [
        [
          "../../transform",
          {
            "blackbox": true,
            "rt": "../apiMin",
            "pureModule": true,
            "preInstrumentedLibs": ".."
          }
        ]
      ]
    }
  ]
}
